{
  "name": "taskmanager-tasks-service",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/juanD1/test_e-volution.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & nodemon dist",
    "devwin": "concurrently --kill-others \"tsc --watch\" \"nodemon src\"",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "start": "node ./dist/index.js",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "=0.19.0",
    "bcryptjs": "=2.4.0",
    "body-parser": "=1.18.2",
    "core-util-is": "^1.0.2",
    "cors": "=2.8.4",
    "express": "=4.15.3",
    "inversify": "=4.13.0",
    "mongodb": "=3.1.13",
    "nodemon": "=1.11.0",
    "reflect-metadata": "=0.1.12",
    "winston": "=2.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "=1.16.8",
    "@types/cors": "=2.8.4",
    "@types/express": "=4.11.1",
    "@types/jest": "=19.2.4",
    "@types/mongodb": "=3.1.14",
    "@types/node": "=7.0.55",
    "@types/winston": "=2.3.8",
    "babel-jest": "=19.0.0",
    "concurrently": "^4.1.0",
    "jest": "=20.0.4",
    "ts-jest": "=19.0.6",
    "typescript": "=2.9.2",
    "typescript-eslint-parser": "=3.0.0"
  },
  "jest": {
    "globals": {
      "__TEST__": "test"
    },
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  },
  "bugs": {
    "url": "https://github.com/juanD1/test_e-volution/issues"
  },
  "homepage": "https://github.com/juanD1/test_e-volution#readme",
  "description": ""
}
